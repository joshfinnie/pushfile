{
  "name": "pushfile",
  "publishConfig": {
    "access": "public"
  },
  "preferGlobal": true,
  "keywords": ["aws", "s3", "uploading", "cli"],
  "version": "4.0.0",
  "type": "module",
  "description": "Command line app that pushes a file to S3 and gives you a URL.",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/helpers.js",
  "types": "dist/helpers.d.ts",
  "bin": {
    "pushfile": "./dist/cli.js"
  },
  "exports": {
    "types": "./dist/cli.d.ts",
    "default": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup --minify",
    "link": "npm link",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "format": "biome check --fix",
    "format:ci": "biome check",
    "ci": "pnpm run test && pnpm run typecheck && pnpm run format:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshfinnie/pushfile.git"
  },
  "author": "Josh Finnie <josh@jfin.us>",
  "contributors": [
    {
      "name": "Josh Finnie",
      "email": "josh@jfin.us"
    },
    {
      "name": "Will Laurance"
    },
    {
      "name": "Matthew Chase Whittemore"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshfinnie/pushfile/issues"
  },
  "homepage": "https://github.com/joshfinnie/pushfile#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "chalk": "^5.6.2",
    "clipboardy": "^5.0.1",
    "commander": "^14.0.2",
    "cosmiconfig": "^9.0.0",
    "figlet": "^1.9.4",
    "hashids": "^2.3.0",
    "inquirer": "^13.0.1",
    "mime-types": "^3.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "4.0.14",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
