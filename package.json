  },
  "homepage": "https://github.com/joshfinnie/pushfile#readme",
  "dependencies": {
    "aws-sdk": "2.597.0",
    "chalk": "3.0.0",
    "clipboardy": "2.1.0",
    "commander": "4.0.1",
    "figlet": "1.2.4",
    "hashids": "2.1.0",
